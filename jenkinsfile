	git branch: 'main', url: 'https://github.com/ggutu/simplePython-microservices-.git'
	}
    }
	stage('docker build image'){
    steps{
	 script{
	  sh  'docker build -t gizachewgutu/devops-integration .'
	   }
	}
    }
   /* stage('docker push'){
